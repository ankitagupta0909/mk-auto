package com.markit.demo;

public class DemoClass_Constructor {

		   public DemoClass_Constructor(String name){
		      // This constructor has one parameter, name.
		      System.out.println("Passed Name is :" + name ); 
		   }
		   
		   public static void main(String []args){
		      // Following statement would create an object cons
			   DemoClass_Constructor cons = new DemoClass_Constructor( "tommy" );
			   
		   }
	}

