package com.markit.demo;

public class LUR {

	public static void main(String[] args){
/*	int arr[]={4,5,1,2,3};
	
	for(int i=0;i<arr.length;i++){
		if(arr[i]>arr[i+1]){
			System.out.println(arr[i]);
